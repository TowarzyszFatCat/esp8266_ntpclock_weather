void setup() {
  // OLED SETUP
  Wire.begin(2,1); 
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
  for(;;);}
  delay(2000);
  display.clearDisplay();

  // PINMODE SETUP
  pinMode(LED_BUILTIN, OUTPUT); //Led płytka
  pinMode(touchButton, INPUT);

  WiFi.begin(ssid, password);
  WiFi.hostname(newHostname.c_str());

  display.clearDisplay();
  display.setCursor(0, 0);
  display.setTextColor(WHITE);
  display.println("Uruchamianie...");
  display.display();
  delay(500);
  display.clearDisplay();
  display.print("---------------------");
  display.println("SSID: " + String(ssid));
  display.println("Hostname: " + newHostname);
  display.println("---------------------");
  display.display();

  while ( WiFi.status() != WL_CONNECTED ) {
    display.setCursor(0, 0);
    display.println("Laczenie z WiFi");
    display.display();
    delay(400);
    display.setCursor(0, 0);
    display.println("Laczenie z WiFi.");
    display.display();
    delay(400);
    display.setCursor(0, 0);
    display.println("Laczenie z WiFi..");
    display.display();
    delay(400);
    display.setCursor(0, 0);
    display.println("Laczenie z WiFi...");
    display.display();
    delay(400);
  }

  if(WiFi.status() == WL_CONNECTED){
    display.setTextSize(1);
    display.setCursor(0, 0);
    display.println(" ");
    display.println(" ");
    display.println(" ");
    display.println(" ");
    display.println(" ");
    display.println("Polaczono!");
    display.display();
    delay(1000);
    ip = WiFi.localIP();
    display.print("IP: ");
    display.print(ip);
    display.display();
    delay(2500);
  }

  timeClient.begin();

  // START DHT11
  dht.begin();
}

void loop() {

  timeClient.update();
  int hour = timeClient.getHours();
  int minute = timeClient.getMinutes();
  int second = timeClient.getSeconds();

  String hourstr = String(hour);
  String minutestr = String(minute);
  String secondstr = String(second);

  if(hourstr.length() == 1){
    hourstr = "0" + hourstr;
  }
  if(minutestr.length() == 1){
    minutestr = "0" + minutestr;
  }
  if(secondstr.length() == 1){
    secondstr = "0" + secondstr;
  }


  #pragma region screenIndex
  touchValue = digitalRead(touchButton);
  if(touchValue){
    screenIndex++;
    if(screenIndex == 2){
      screenIndex = 0;
    }
    delay(500);
  }
  #pragma endregion

  if(screenIndex == 0){
    display.clearDisplay();
    display.setCursor(0, 0);
    display.setTextColor(WHITE);
    display.drawBitmap(0, 0, amongus_left, 128, 64, WHITE);
    display.setTextSize(2);
    display.setCursor(50,10);
    display.print(hourstr + ":" + minutestr);
    display.setTextSize(1);
    display.print(":" + secondstr);
    display.setTextSize(1);
    display.setCursor(53,40);
    display.println("Temp: " + String(getTemp()));
    display.setCursor(53,48);
    display.println("Wilg: " + String(getHum()));
    display.display();
  }

  if(screenIndex == 1){
    time_t epochTime = timeClient.getEpochTime();
    String formattedTime = timeClient.getFormattedTime();
    String weekDay = weekDays[timeClient.getDay()];
    struct tm *ptm = gmtime ((time_t *)&epochTime);
    int monthDay = ptm->tm_mday;
    int currentMonth = ptm->tm_mon+1;
    String monthDayStr = String(monthDay);
    String currentMonthStr = String(currentMonth);
    String currentMonthName = months[currentMonth-1];
    int currentYear = ptm->tm_year+1900;
    String currentDate = String(weekDay) + "-" + String(currentMonthName) + "-" + String(currentYear);

    if(monthDayStr.length() == 1){
      monthDayStr = "0" + monthDayStr;
    }
    if(currentMonthStr.length() == 1){
      currentMonthStr = "0" + currentMonthStr;
    }

    display.clearDisplay();
    display.drawBitmap(0, 0, kurwinox, 128, 64, WHITE);
    display.setCursor(64, 0);
    display.setTextSize(3);
    display.setTextColor(WHITE);
    display.print(monthDayStr);
    display.setTextSize(2);
    display.print(weekDay);
    display.println(" ");
    display.println("       " + String(currentMonthName));
    display.println("      " + String(currentYear) + "r");
    display.display();
  }

  if(screenIndex == 2){
    display.clearDisplay();
    display.setTextSize(2);
    display.setCursor(0, 0);
    display.setTextColor(WHITE);
  }

  if(screenIndex == 3){
    display.clearDisplay();
    display.setTextSize(4);
    display.setCursor(0, 0);
    display.setTextColor(WHITE);
    display.drawBitmap(0, 0, aaa, 128, 64, WHITE);
    display.display();
  }

}
